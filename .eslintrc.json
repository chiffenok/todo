{
    "env": {
        "browser": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "extends": "airbnb",
    "rules": {
        "indent": ["error", 4],
        "max-len": [1, 120, 2, {"ignoreComments": true}],
        "react/prop-types": ["warn"],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}